<?xml version="1.0"?>
<package >
  <metadata>
    <id>Koben.Iconic</id>
    <version>2.3.3</version>
    <title>Iconic</title>
    <authors>Mario Lopez</authors>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/KOBENDigital/Iconic</projectUrl>
    <icon>images\iconic_logo.png</icon>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>Icon picker for Umbraco</summary>
    <description>
      <![CDATA[Highly configurable icon picker for Umbraco. With Iconic you will be able to create a property editor that can use virtually any font package out there and not only that, you will be able to select your icon from more than one package on the same property.
    ]]>
    </description>
    <releaseNotes>
      <![CDATA[
      #23 Use UniqueKey to avoid aliases conflicts.
      #25 Add to some Files the Virtual Path.      
    ]]>
    </releaseNotes>
    <tags>Iconic Umbraco icon picker editor backoffice</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.7.2">
        <dependency id="UmbracoCms.Core" version="[8.0,]" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="images\iconic_logo.png" target="images"/>
    <file src="App_Plugins\Iconic\**\*.*" target="content\App_Plugins\Iconic"/>
    <file src="App_Plugins\Iconic\js\build\*.*" target="content\App_Plugins\Iconic\js\build" />
    <file src="App_Plugins\Iconic\styles\*.*" target="content\App_Plugins\Iconic\styles" />
    <file src="bin\Release\Koben.Iconic.*" target="lib\net472"></file>
  </files>
</package>
