    "/App_Plugins/Iconic/js/build/iconic.dialog.controller.js",
    "/App_Plugins/Iconic/js/build/iconic.prevalues.controller.js",
    "/App_Plugins/Iconic/js/build/iconic.directives.js",
    "/App_Plugins/Iconic/js/build/iconic.prevalues.editor.controller.js"
  ],
  "css": [
    "/App_Plugins/Iconic/styles/iconic.min.css"
